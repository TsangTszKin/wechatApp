.top {
  height: 20vh;
  background-color: #fb6462;
  margin: 10px 20px;
  border-radius: 15px;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.top .left {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.top .left .top1 {
  color: #fff;
  height: 8vh;
  display: flex;
  justify-content: center;
  align-items: center;
}

.top .left .bottom1 {
  display: flex;
  justify-content: center;
  height: 12vh;
}

.top .left .bottom1 .cell {
  height: 50px;
  width: 30px;
  color: #fb6462;
  line-height: 50px;
  font-size: 30px;
  background-color: #fff;
  border-radius: 5px;
  margin: 0 3px;
  text-align: center;
  font-weight: bold;
}

.top .right {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.top .right .top2 {
  color: #fff;
  height: 8vh;
  display: flex;
  justify-content: center;
  align-items: center;
}

.top .right .bottom2 {
  display: flex;
  justify-content: center;
  height: 12vh;
}

.top .right .bottom2 .cell {
  height: 50px;
  width: 30px;
  color: #fb6462;
  line-height: 50px;
  font-size: 30px;
  background-color: #fff;
  border-radius: 5px;
  margin: 0 3px;
  text-align: center;
  font-weight: bold;
}

.sign-btn {
  color: #fff;
  background-color: #fb6462;
  border-color: #fb6462;
}

.body {
  width: 86vw;
  height: 86vw;
  margin: 7vw;
  border-radius: 8px;
  color: #fff;
  background: url('data:image/jpg;base64,') no-repeat center;
  background-size: 86vw 86vw;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.body .title1 {
  font-size: 27px;
}

.body .title2 {
  font-size: 20px;
}

.body .title3 {
  font-size: 12px;
}

<view class="container more" style='display: block;'>
  <view class="user flex-wrp">
    <view class="avatar flex-item">
      <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
    </view>
    <view class="user-info flex-item">
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
      <text class="edit">查看或编辑个人主页</text>
    </view>
  </view>

  <view class="my">
    <navigator url="/pages/my-wallet/my-wallet">
      <view class="my-item flex-wrp">
        <view class="myitem-icon flex-item">
          <image src="../../images/icon-wallet.png"></image>
        </view>
        <view class="myitem-name flex-item">
          <text>我的卡包</text>
        </view>
      </view>
    </navigator>


    <view class="my-item flex-wrp">
      <view class="myitem-icon flex-item">
        <image src="../../images/icon-service.png"></image>
      </view>
      <view class="myitem-name flex-item">
        <text>我的服务</text>
      </view>
    </view>

    <view class="my-item flex-wrp">
      <view class="myitem-icon flex-item">
        <image src="../../images/icon-activity.png"></image>
      </view>
      <view class="myitem-name flex-item">
        <text>我的活动</text>
      </view>
    </view>

    <view class="my-item flex-wrp">
      <view class="myitem-icon flex-item">
        <image src="../../images/icon-collect.png"></image>
      </view>
      <view class="myitem-name flex-item">
        <text>我的收藏</text>
      </view>
    </view>

    <navigator url="/pages/message/message">
      <view class="my-item flex-wrp">
        <view class="myitem-icon flex-item">
          <image src="../../images/icon-collect.png"></image>
        </view>
        <view class="myitem-name flex-item">
          <text>消息</text>
        </view>
      </view>
    </navigator>

    <navigator url="/pages/sign/sign">
      <view class="my-item flex-wrp">
        <view class="myitem-icon flex-item">
          <image src="../../images/icon-collect.png"></image>
        </view>
        <view class="myitem-name flex-item">
          <text>每日1签</text>
        </view>
      </view>
    </navigator>


    <view class="my-item flex-wrp">
      <view class="myitem-icon flex-item">
        <image src="../../images/icon-collect.png"></image>
      </view>
      <view class="myitem-name flex-item">
        <text>养生打卡</text>
      </view>
    </view>

  </view>
</view>
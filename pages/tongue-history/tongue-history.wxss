.page {
  padding-left: 30px;
}

.img-cell {
  padding: 10px 10px 50px 10px;
  /* border-bottom: 1px solid gainsboro;
  border-top: 1px solid gainsboro; */
  height: auto;
  width: auto;
  display: block;
  border-left: 1px solid #a46a3e;
}

.img-cell .header {
  position: relative;
  margin-bottom: 20px;
  height: 30px;
  left: -20px;
}

.img-cell .header .status {
  position: absolute;
  right: 10px;
  top: 0;
}

.img-cell .header .title-o-o {
  width: 12px;
  height: 12px;
  border-radius: 50%;
  border: 1px solid #a46a3e;
  padding: 2px;
  position: absolute;
  top: 7px;
}

.img-cell .header .title-o-o .title-o {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background-color: #a46a3e;
}

.img-cell .header .title {
  font-size: 15px;
  word-break: break-all;
  /* display: -webkit-box; */
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  /* letter-spacing: 2px; */
  line-height: 15px;
  padding: 8px 0;
  width: calc(100% - 65px);
  position: absolute;
  left: 30px;
  top: 0;
}

.img-cell  .body {
  display: flex;
  justify-content: space-around;
  justify-items: center;
}

.img-cell  .body .content {
  text-align: center;
}

.img-cell  .body  .content .content-2 {
  display: flex;
  justify-content: center;
  overflow: hidden;
  align-items: center;
  width: 100px;
  height: 100px;
}

.img-cell  .body .content .content-2 .img {
  /* width: 100%; */
}
